# URL base da B3 (pode ser sobrescrito via variável de ambiente)
B3_BASE_URL=https://sistemaswebb3-listados.b3.com.br

# Caminho para dados do IBOV
IBOV_PATH=/indexPage/day/IBOV

# Número de registros por página para o endpoint JSON (default 120)
PAGE_SIZE=120

# Bucket S3 para persistência dos dados (obrigatório)
S3_BUCKET=fiap-bovespa-rawdata

# Credenciais de acesso AWS
AWS_ACCESS_KEY_ID=<YOUR_AWS_ACCESS_KEY_ID>
AWS_SECRET_ACCESS_KEY=<YOUR_AWS_SECRET_ACCESS_KEY>

# Região AWS (opcional, default us-east-1)
AWS_REGION=us-east-1

# ARN do IAM Role que o scraper deve assumir
AWS_ROLE_ARN=arn:aws:iam::914261088296:role/LabRole

# Timeout de requisição em segundos (opcional, default 10)
TIMEOUT=10

# Prefixo de pasta no bucket S3 (opcional, default raw/ibov)
S3_PREFIX=raw/ibov